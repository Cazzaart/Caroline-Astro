---
import Layout from "@layouts/BaseLayout.astro";
const { title, description } = Astro.props;
---

<Layout title={title} description={description}>
  <main class="standard-content-width">
    <article>
      <header>
        <h1>{title}</h1>
      </header>
      <slot />
    </article>
  </main>
</Layout>
